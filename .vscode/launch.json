{
	"version": "0.2.0",
	"configurations": [

		{
			"type": "totvs_language_debug",
			"request": "launch",
			"name": "TOTVS Language Debug",
			"program": "${command:AskForProgramName}",
			"cwb": "${workspaceFolder}",
			"smartclientBin": "../totvs/bin/smartclient/smartclient.exe",
			"isMultiSession": true,
			"enableTableSync": true,
			"extendFeatures": {
				"charDetails": false
			}
		},
		{
			"type": "pwa-chrome",
			"request": "launch",
			"name": "Launch Chrome",
			"url": "http://127.0.0.1:9001/index.html",
			"webRoot": "${workspaceFolder}"
		}
	]
}